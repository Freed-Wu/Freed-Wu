StrictHostKeyChecking no

Host github.com
  User git

# https://docs.github.com/cn/authentication/troubleshooting-ssh/using-ssh-over-the-https-port
Host ssh.github.com
  User git
  Port 443

Host aur.archlinux.org
  User aur

Host localhost
  UserKnownHostsFile /dev/null

Host docker
  HostName localhost
  UserKnownHostsFile /dev/null
  Port 8022

Host arch
  User wzy

Host desktop
  User wzy
  HostName 192.168.151.188

Host ustc
  HostName 202.38.69.241
  User wuzy
  Port 39099
