set auto-load safe-path /
set history filename ~/.cache/gdbinit
set history save on
set print array on
set output-radix 16
set logging overwrite on
set disable-randomization on
set confirm off
set extended-prompt \[\e[37;44m  \w \e[34;40m\e[37m  \f \e[30;43m\e[30m  \t \e[0;33m\e[0m\]\n❯ 

define arg
  print *argv @ argc
end

define pp
  set $i = 0
  while $i < $argc
    eval "print $arg%d", $i
    set $i += 1
  end
end
