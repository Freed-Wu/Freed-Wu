set auto-load safe-path /
set history filename ~/.cache/gdbinit
set history save on
set print array on
set output-radix 16
set logging overwrite on
set disable-randomization on
set confirm off
source ~/.config/gdb/gdbinit.py

define arg
  print *argv @ argc
end

define pp
  set $pp_i = 0
  while $pp_i < $argc
    eval "print $arg%d", $pp_i
    set $pp_i += 1
  end
end
