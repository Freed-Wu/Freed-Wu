{ lib, stdenv, fetchFromGitHub }:
stdenv.mkDerivation rec {
  pname = "{{ expand('%:p:h:t') }}";
  version = "";

  src = fetchFromGitHub {
    owner = "";
    repo = "{{ expand('%:p:h:t') }}";
    rev = version;
    sha256 = "";
  };

  dontBuild = true;
  installFlags = [ "PREFIX=$(out)" ];

  meta = with lib; {
    homepage = "https://github.com//{{ expand('%:p:h:t') }}";
    description = "";
    license = licenses.mit;
    maintainers = with maintainers; [ ];
    platforms = platforms.unix;
  };
}
